= simple_form_for @help, url: { controller: "web/admin/helps", action: action }, html: { class: 'form-horizontal' } do |f|
  = f.input :title
  = f.input :body
  .form-actions
    = f.button :submit, class: 'btn-primary'
    = link_to t('.cancel', default: t("helpers.links.cancel")), admin_helps_path, class: 'btn'
